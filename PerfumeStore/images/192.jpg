<script type="text/javascript">
        jQuery('.out_of_stock_toggle').change(function() {
            // get selected value
            var select_option = jQuery("input[name='out_of_stock_toggle']:checked").val();

            // delete cookie
            // jQuery.cookie('out_of_stock_toggle_flag', 0, { path: '/' });

            // set option value
            jQuery.cookie('out_of_stock_toggle_flag', select_option, {
                expire: 1,
                path  : '/',
                domain: '.luekensliquors.com',
                secure: true
            });

            // reload page
            window.location.href = count_path_parameters();
        });

        // count path parameters
        function count_path_parameters()
        {
            // get current location
            var curr_location = window.location.href;

            // define array
            var param_arr = new Array();

            // define redirect url
            var redirect_url = window.location.origin + window.location.pathname + '/?page=load';

            // check parameters in path
            if(curr_location.indexOf('?') > -1)
            {
                // split parameters using &
                var params = curr_location.substr(curr_location.indexOf('?') + 1, curr_location.length).split("&");

                // fetch parameters
                for(var i = 0; i < params.length; i++)
                {
                    // split parameter using qual to
                    var propVal   = params[i].split("=");
                    var paramName = propVal[0];
                    var value     = propVal[1];

                    // update redirect url
                    if(paramName != 'page' && value != 'load') {
                        redirect_url = redirect_url + '&' + paramName + '=' + value;
                    }
                }
            }

            // return response
            return redirect_url;
        }
    </script>
