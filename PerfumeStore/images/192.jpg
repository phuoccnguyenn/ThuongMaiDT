<script type="text/javascript">

            jQuery(document).ready(function ($) {
                console.log('product single page');
                "use strict";

                $('.single_add_to_cart_button').click(function (e) {
                    e.preventDefault();
                    var id = $(this).next().next().attr('value');
                    var stock = '0';
                    var qty = $(this).parent().find(".qty").val();
                    var name=$('h1.product-title').text();
                    var data = {
                        product_id: id,
                        product_name: name,
                        quantity: qty,
                        stock: stock
                    };
                    $(this).addClass('loading');
                    $.post(wc_add_to_cart_params.wc_ajax_url.toString().replace('%%endpoint%%', 'add_to_cart'), data, function (response) {

                            if (!response) {
                                return;
                            }
                            if (response.error) {
                               if($('div.woocommerce-notices-wrapper span.add-cart-error').length){
                                    $('div.woocommerce-notices-wrapper span.add-cart-error').remove();
                                }

                                
                                $('div.woocommerce-notices-wrapper').addClass('alert-color medium-text-center');
                                $('ul.woocommerce-error.message-wrapper').remove();
                                $('.content-area .site-main').css('padding-top','15px');
                                $('div.woocommerce-notices-wrapper').html('<span class="add-cart-error"><b style="font-weight: 700; color:#9B2743; font-size: 1.1em;">You cannot add that amount of "'+name.trim()+'" to the cart because there is not enough stock ('+stock.split('.')[0]+' remaining).</b></span>');
                                
                                if($('div#login-form-popup div.woocommerce-notices-wrapper span.add-cart-error').length){
                                    $('div#login-form-popup div.woocommerce-notices-wrapper span.add-cart-error').remove();
                                }

                                $('.single_add_to_cart_button').removeClass('loading');
                                return;
                            }
                            if (response) {

                                var url = woocommerce_params.wc_ajax_url;
                                url = url.replace("%%endpoint%%", "get_refreshed_fragments");
                                $.post(url, function (data, status) {
                                    $(".woocommerce.widget_shopping_cart").html(data.fragments["div.widget_shopping_cart_content"]);
                                    if (data.fragments) {
                                        jQuery.each(data.fragments, function (key, value) {

                                            jQuery(key).replaceWith(value);
                                        });
                                    }
                                    jQuery("body").trigger("wc_fragments_refreshed");
                                });
                                $('.single_add_to_cart_button').removeClass('loading');
                                $('.header-nav').find('.cart-item').addClass('current-dropdown');

                            }
                        });

                });
            });

        </script>
